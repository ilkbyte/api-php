{
    "name": "netinternet/ilkbyte",
    "description": "Laravel ilkbyte package",
    "keywords": [
        "laravel",
        "server",
        "domain",
        "dns"
    ],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Umut Aktepe",
            "email": "umtaktpe@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": ">=5.4",
        "illuminate/console": ">=5.4",
        "illuminate/filesystem": ">=5.4",
        "symfony/class-loader": "^2.3|^3.0"
    },
    "require-dev": {
        "illuminate/config": ">=5.4",
        "illuminate/view": ">=5.4",
        "guzzlehttp/guzzle": "~6.0",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Netinternet\\Ilkbyte\\": "src"
        },
        "files" : [
            "src/Helpers/functions.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Netinternet\\Ilkbyte\\IlkbyteServiceProvider"
            ],
            "aliases": {
                "Ilkbyte": "Netinternet\\Ilkbyte\\Facades\\Ilkbyte"
            }
        }
    }

}
